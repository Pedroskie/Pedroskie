# Dwolla ACH environment variables
DWOLLA_APP_KEY=dwolla_app_key_here
DWOLLA_APP_SECRET=dwolla_app_secret_here
DWOLLA_ENVIRONMENT=sandbox
DWOLLA_WEBHOOK_SECRET=replace_with_webhook_secret

# FlowLedger anchoring service
FLOW_LEDGER_ENDPOINT=https://api.flowledger.example/anchor
FLOW_LEDGER_API_TOKEN=replace_with_flowledger_token

# Pipeline and database configuration
GTP5_PIPELINE_ID=gtp5-production
DATABASE_URL=postgresql+psycopg2://dwolla:dwolla@localhost:5432/dwolla
REDIS_URL=redis://localhost:6379/0

# Observability and logging
LOG_LEVEL=INFO
SENTRY_DSN=
